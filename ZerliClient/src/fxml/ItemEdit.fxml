            </Button>
            <Button fx:id="delete" mnemonicParsing="false" onMouseClicked="#btnDelete" prefHeight="47.0" prefWidth="105.0" stylesheets="@../../images/css/styleNew.css" text="Delete" textFill="#004d00">
